{
  "expo": {
    "name": "ExpertOption",
    "slug": "ExpertOption",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/spalsh.png",
      "resizeMode": "contain",
      "backgroundColor": "#1F2A40"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier":"com.raskan.ExpertOption"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#1F2A40"
      },
      "backgroundColor": "#1F2A40",
      "package":"com.raskan.ExpertOption"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1fe6b6ef-b574-48fe-afcd-f19e458839b8"
      }
    }
  }
}
